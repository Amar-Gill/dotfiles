[[ ! -f $HOME/.cargo/env ]] || . "$HOME/.cargo/env"

export GOPATH=$HOME/go
export PATH=$PATH:$GOPATH/bin

export NVM_DIR="$HOME/.nvm"

export PATH="$HOME/.rbenv/shims:${PATH}"

export KUBECTL_ZSH_COMPLETION_PATH=$HOME/.kubectl_zsh_completion # kubectl completionn

export LSCOLORS=Gxfxcxdxbxegedabagacad # https://geoff.greer.fm/lscolors/

# auto generated by pnpm
export PNPM_HOME="/Users/$(whoami)/Library/pnpm"
case ":$PATH:" in
  *":$PNPM_HOME:"*) ;;
  *) export PATH="$PNPM_HOME:$PATH" ;;
esac

export DENO_INSTALL="/Users/$(whoami)/.deno"
export PATH="$DENO_INSTALL/bin:$PATH"
